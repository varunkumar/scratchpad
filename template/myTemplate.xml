body#layout .hide{display:none;}
body#layout #featured{height:60px;}
#navigation a.selected{ text-decoration:none;background: url(http://varun-scratchpad.googlecode.com/svn/trunk/images/zipqut.jpg) bottom center no-repeat;}
#navigation a{text-decoration:none;}

/* Label Cloud Styles
----------------------------------------------- */
#labelCloud1 {text-align:center;font-family:arial,sans-serif;}
#labelCloud1 .label-cloud li{display:inline;background-image:none !important;padding:0 5px;margin:0;vertical-align:baseline !important;border:0 !important;}
#labelCloud1 ul{list-style-type:none;margin:0 auto;padding:0;}
#labelCloud1 a img{border:0;display:inline;margin:0 0 0 3px;padding:0}
#labelCloud1 a{text-decoration:underline;color:#6699CC;}
#labelCloud1 a:hover{text-decoration:underline;color:#669933;}
#labelCloud1 li a{}
#labelCloud1 .label-cloud {}
#labelCloud1 .label-count {padding-left:0.2em;font-size:9px;color:#000}
#labelCloud1 .label-cloud li:before{content:"" !important} 
