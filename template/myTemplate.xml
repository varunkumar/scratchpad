  padding:10px 0;
  overflow:hidden; 
}
#main-wrapper {
  width:75%;
  float:left;
  word-wrap:break-word; 
  overflow:hidden;
  border-left:1px solid #DDDDDD;
  border-right:1px solid #DDDDDD;
  background:url(http://varun-scratchpad.googlecode.com/svn/trunk/static/tile_left.gif) repeat-y;
}

#sidebar-wrapper {
  width:24%;
  padding:0;
  float:right;
  word-wrap:break-word;
  overflow:hidden;
  border-right:1px solid #DDDDDD;
}
#feedlnks{
  margin:0;
  padding:0;
  border-bottom:1px solid #e7e7e7;
}
#feed-links{
 padding:0 0 10px;
